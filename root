			ttd0 = '<tr><th>%rank%</th><td><a href="http://finance.sina.com.cn/realstock/company/%symbol%/nc.shtml" target="_blank" title="%cname%">%scname%</a></td><td class="%ccolor%">%price%</td><td class="%ccolor%">%chg%%</td><td>%ratio%%</td><td><span class="table01_bar"><a href="http://guba.sina.com.cn/?s=bar&name=%symbol%" target="_blank">吧</a></span><span class="table01_tou"><a href=" https://touzi.sina.com.cn/public/stock/analysis/%symbol%" target="_blank" suda-uatrack="key=touziyi&value=stock_home">诊</a></span></td></tr>',
