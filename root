<script>

	(function($){

		var cookie_name_l='fin_stock_lclose',cookie_name_r='fin_stock_rclose',stock_domain='finance.sina.com.cn';

		left_close=FN.cookie.get(cookie_name_l);

		right_close=FN.cookie.get(cookie_name_r);

		if(left_close)

		{

			$('.blk_APP_JSl').hide();



		}

		if(right_close)

		{

			$('.blk_APP_JSr').hide();

		}

		$('.side-btns-APP-close').on('click',function(){

			FN.cookie.set(cookie_name_l,'1',1*24,stock_domain);

			$('.blk_APP_JSl').hide();

		});

		$('.side-btns-APP-r-close').on('click',function(){

			FN.cookie.set(cookie_name_r,'1',1*24,stock_domain);

			$('.blk_APP_JSr').hide();

		});

	})(jQuery);
</script>
