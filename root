			},
			formatC4 = function(){
				var ret = [],
					s = window['hq_json_new_all_turnoverrate'];
				if(typeof s === 'undefined' || s === ''){return ret;}
				for(var i = 0, l = s.length; i < l; i++){
					var d = s[i],
						c = d[5] - 0,
