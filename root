						c = d[9] - 0,
						hc = d[5] - 0,
						ccolor = c > 0 ? 'cred' : c < 0 ? 'cgreen' : '',
						hccolor = hc > 0 ? 'cred' : hc < 0 ? 'cgreen' : '';
					ret.push({
						rank : i + 1,
						symbol : d[8],
						cname : d[12],
						scname : d[12].slice(0,5),
						chg : c.toFixed(2),
						ccolor : ccolor,
						hcode : d[0],
						hcname : d[1],
						shcname : d[1].slice(0,5),
						hchg : hc.toFixed(2),
						hccolor : hccolor
					});
				}
				paint('p02_l01_c02',ret,tth4,ttd1);
			},
			formatC7 = function(){
				var ret = [],
					s = window['hq_json_si_api2'];
				if(typeof s === 'undefined' || s === ''){return ret;}
				for(var i = 0, l = s.length; i < l; i++){
					var d = s[i],
						hc = d['avg_changeratio'] * 100,
						hccolor = hc > 0 ? 'cred' : hc < 0 ? 'cgreen' : '';
					ret.push({
						rank : i + 1,
						symbol : d['ts_symbol'],
						cname : d['ts_name'],
						scname : d['ts_name'].slice(0,5),
						hcode : d['category'],
						hcname : d['name'],
						shcname : d['name'].slice(0,5),
						hchg : hc.toFixed(2),
						hnet : FN.kNum(d['netamount'] / 1e4,2),
						hccolor : hccolor
					});
				}
				paint('p02_l01_c03',ret,tth5,ttd2);
			},
