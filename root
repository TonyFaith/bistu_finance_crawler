				var ret = [],
					s = window['hq_json_si_api4'];
				if(typeof s === 'undefined' || s === ''){return ret;}
				for(var i = 0, l = s.length; i < l; i++){
					var d = s[i],
						c = d['ts_changeratio'] * 100,
						hc = d['avg_changeratio'] * 100,
						ccolor = c > 0 ? 'cred' : c < 0 ? 'cgreen' : '',
						hccolor = hc > 0 ? 'cred' : hc < 0 ? 'cgreen' : '';
					ret.push({
						rank : i + 1,
						symbol : d['ts_symbol'],
						cname : d['ts_name'],
						scname : d['ts_name'].slice(0,5),
						hcode : d['category'],
						hcname : d['name'],
						shcname : d['name'].slice(0,5),
						hchg : hc.toFixed(2),
						chg : c.toFixed(2),
						ccolor : ccolor,
						hccolor : hccolor
					});
				}
				paint('p02_l01_c05',ret,tth3,ttd1);
			},
			formatC10 = function(){
				var ret = [],
					s = window['hq_json_si_api5'];
