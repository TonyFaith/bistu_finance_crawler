			formatC13 = function(){
				var ret = [],
					symbols = [],
					s = window['hq_json_si_api8'];
				if(typeof s === 'undefined' || s === ''){return ret;}
				s = s.data || [];
				for(var k = 0, kl = s.length; k < kl; k++){
					symbols.push(s[k]['symbol']);
				}
				getDatas(hqapi + (function(){
					var rets = [];
					for(var n = 0, nl = symbols.length; n < nl; n++){
						rets.push('s_' + symbols[n]);
					}
					return rets.join(',');
				})(),function(){
					for(var i = 0, l = s.length; i < l; i++){
						var d = s[i],
							symbol = d['symbol'],
							sd = (window['hq_str_s_' + symbol] || '').split(','),
							c = sd['3'] - 0,
							ccolor = c > 0 ? 'cred' : c < 0 ? 'cgreen' : '';
						ret.push({
							symbol : d['symbol'].replace(/[^\d]/g,''),
							cname : d['symbol_name'],
							scname : d['symbol_name'].slice(0,5),
							ccolor : ccolor,
							chg : c.toFixed(2),
							atprice : FN.kNum(d['avg_targetprice'] - 0,2),
							atchg : (d['avg_incpercent'] - 0).toFixed(2),
							price : FN.kNum(sd[1] - 0,2)
						});
					}
					paint('p03_r01_c01',ret,tth7,ttd3);
				});
			},
