			tth3 = '<thead><tr><th>排名</th><th>名称</th><th>涨幅</th><th>领涨股</th><th>涨幅</th><th>&nbsp;</th></tr></thead>',
			tth4 = '<thead><tr><th>排名</th><th>名称</th><th>涨幅</th><th>领跌股</th><th>涨幅</th><th>&nbsp;</th></tr></thead>',
			tth5 = '<thead><tr><th>排名</th><th>名称</th><th>涨幅</th><th>净流入(万元)</th><th>领涨股</th><th>&nbsp;</th></tr></thead>',
