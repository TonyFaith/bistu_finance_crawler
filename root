	//tab-p01_r01
	(function(){
		var etype = isPAD ? 'onclick' : 'onmouseover',
			ssc = new SubShowClass('none',etype,'','selected');
		ssc.addLabel('p01_r01_lab01','p01_r01_c01');
		ssc.addLabel('p01_r01_lab02','p01_r01_c02');
	})();

	//recent&hot-p01_r01
	(function(){
		var w = document.getElementById('p01_r01_c01'),
			btn = document.getElementById('p01_r01_lab01_change'),
			hqapi = 'http://hq.sinajs.cn/ran=%ran%&list=%list%',
			hotapi = 'http://finance.sina.com.cn/realstock/company/hotstock_daily_a.js',
			delay = 6e3,
			num = 10,
			start = 0,
			interval = null,
			recents = [],
			hots = [],
			cooname = 'FINA_V_S_2',
			tls = '<ul class="list02">',
			tle = '</ul>',
			tlh = '<li><p class="list02_h">以下为热门股票</p></li>',
			tli = '<li %func%><a href="http://finance.sina.com.cn/realstock/company/%symbol%/nc.shtml" target="_blank" class="list02_name">%cname%</a><span class="list02_diff %ccolor%">%price%</span><span class="list02_chg %ccolor%">%chg%</span><span class="list02_tip"><a href="http://guba.sina.com.cn/?s=bar&name=%symbol%" target="_blank" class="ba">吧</a><a href=" https://touzi.sina.com.cn/public/stock/analysis/%symbol%" target="_blank" suda-uatrack="key=ad_pc_finance_stock_index&value=ad_pc_finance_stock_index_03" class="zhen">诊</a></span><span class="list02_del" symbol="%symbol%" title="删除"></span></li>',
			tfunc = 'onmouseover="FN.addClass(this,\'list02_hover\')" onmouseout="FN.removeClass(this,\'list02_hover\')"',
			getRecents = function(){
				var rcoo = FN.cookie.get(cooname);
				if(rcoo !== ''){recents = rcoo.split(',');}
				else{recents = [];}
				getHots();
			},
			getHots = function(){
				FN.getScript(hotapi,function(){
					var datas = window['hotstock_daily_a'] || [],
						l = datas.length,
						orec = FN.arrToObj(recents);
					hots = [];
					for(var i = 0, l = datas.length; i < l; i++){
						var d = datas[i][0];
						if(typeof d !== 'undefined' && typeof orec[d] === 'undefined'){
							hots.push(d);
						}
					}
					initStocks();
				});
			},
			getStocks = function(url){
				FN.getScript(url.replace(/%ran%/,Math.random()),function(){
					paint();
				});
			},
			paint = function(){
				var h = [],
					symbols = recents.concat(['tlh']).concat(hots),
					l = symbols.length,
					lr = recents.length,
					i = 0,k = 0;
				h.push(tls);
				if(lr < start && symbols[start] !== 'tlh'){
					h.push(tlh);
					k++;
				}
				for(i = start; i < l, k < num; i++, k++){
					var d,s,ttli = tli,
						symbol,cname,price,chg,ccolor;
					if(i < lr){ttli = ttli.replace(/%func%/,tfunc);}
					else if(symbols[i] === 'tlh' && k === (num - 1)){break;}
					else{ttli = ttli.replace(/%func%/,'');}
					symbol = symbols[i];
					if(symbol === 'tlh'){
						h.push(tlh);
						continue;
					}
					d = window['hq_str_s_' + symbol];
					if(!d){
						cname = symbol;
						price = '--';
						chg = '--';
						ccolor = '';
					}else{
						s = d.split(',');
						cname = s[0];
						price = (s[1] - 0).toFixed(2);
						chg = s[3] - 0;
						ccolor = chg > 0 ? 'cred' : chg < 0 ? 'cgreen' : '';
						chg = chg > 0 ? '+' + chg.toFixed(2) + '%' : chg.toFixed(2) + '%';
					}
					h.push(ttli.replace(/%symbol%/g,symbol).replace(/%cname%/g,cname).replace(/%ccolor%/g,ccolor).replace(/%price%/g,price).replace(/%chg%/g,chg));
				}
				h.push(tle);
				w.innerHTML = h.join('');
			},
			initStocks = function(){
				var symbols = recents.concat(hots),
					url = hqapi.replace(/%list%/,(function(){
						var ret = [];
						for(var i = 0, l = symbols.length; i < l; i++){
							ret.push('s_' + symbols[i]);
						}
						return ret.join(',');
					})());
				getStocks(url);
			},
			init = function(){
				getRecents();
				interval && clearInterval(interval);
				interval = setInterval(function(){
					getRecents();
				},delay);
			};
		FN.addEvent(btn,'click',function(e){
			FN.preventDefault(e);
			FN.stopPropagation(e);
			if(start === 0){start = num;}
			else{start = 0;}
			init();
		});
		FN.delegate(document.getElementById('p01_r01_c01'),'list02_del','click',function(e,t){
			var p = FN.getParByTag(t,'li'),
				symbol = t.getAttribute('symbol') || '',
				coo = FN.cookie.get(cooname),
				ret = [];
			p.parentNode.removeChild(p);
			if(coo === ''){return;}
			coo = coo.split(',');
			for(var i = 0, l = coo.length; i < l; i++){
				if(coo[i] === symbol){continue;}
				ret.push(coo[i]);
			}
			FN.cookie.set(cooname,ret.join(','),365 * 24,'finance.sina.com.cn');
			init();
		});
		init();
	})();

	//minestocks-p01_r01
	(function(){
		var w = document.getElementById('p01_r01_c02_list'),
			hqapi = 'http://hq.sinajs.cn/ran=%ran%&list=%list%',
			hotapi = 'http://finance.sina.com.cn/realstock/company/hotstock_daily_a.js',
			mylistapi = 'http://stock.finance.sina.com.cn/portfolio/api/jsonp.php/readMystocks/PortfolioService.getPyList?type=cn',
			delay = 6e3,
			mystocksapi = 'http://stock.finance.sina.com.cn/portfolio/api/jsonp.php/makeMyStocks/HoldService.getSymbolListByPid?pid=%pid%&type=cn',
			delmystockapi = 'http://stock.finance.sina.com.cn/portfolio/api/json.php/HoldService.delAttentionSymbol?pid=%pid%&slist=%symbol%&type=cn',
			pid,
			num = 10,
			interval = null,
			minestocks = [],
			hots = [],
			cooname = 'SINA_PORTFOLIO',
			tls = '<ul class="list02">',
			tle = '</ul>',
			tlh = '<li><p class="list02_h">以下为热门股票</p></li>',
			tli = '<li %func%><a href="http://finance.sina.com.cn/realstock/company/%symbol%/nc.shtml" target="_blank" class="list02_name">%cname%</a><span class="list02_diff %ccolor%">%price%</span><span class="list02_chg %ccolor%">%chg%</span><span class="list02_tip"><a href="http://guba.sina.com.cn/?s=bar&name=%symbol%" target="_blank" class="ba">吧</a><a href="https://touzi.sina.com.cn/public/stock/analysis/%symbol%" target="_blank" suda-uatrack="key=ad_pc_finance_stock_index&value=ad_pc_finance_stock_index_04" class="zhen">诊</a></span><span class="list02_del" symbol="%symbol%" title="删除"></span></li>',
			tfunc = 'onmouseover="FN.addClass(this,\'list02_hover\')" onmouseout="FN.removeClass(this,\'list02_hover\')"',
			getMinestocks = function(){
				minestocks = [];
				hots = [];
				FN.getScript(mylistapi);
			},
			readMystocks = function(s){
				if(!s || !s[0] || !s[0]['portfolio_id']){getHots();}
				else{
					pid = s[0]['portfolio_id'];
					FN.getScript(mystocksapi.replace(/%pid%/,pid));
				}
			},
			makeMyStocks = function(s){
				if(!s){getHots();}
				else{
					minestocks = [];
					for(var i = 0, l = s.length; i < l; i++){
						minestocks.push(s[i]['market'] + s[i]['code']);
					}
					getHots();
				}
			},
			getHots = function(){
				FN.getScript(hotapi,function(){
					var datas = window['hotstock_daily_a'] || [],
						l = datas.length,
						rl = minestocks.length,
						orec = FN.arrToObj(minestocks);
					for(var i = 0, l = datas.length; i < l; i++){
						var d = datas[i][0];
						if(typeof d !== 'undefined' && typeof orec[d] === 'undefined'){
							rl ++;
							hots.push(d);
						}
					}
					initStocks();
				});
			},
			getStocks = function(url){
				FN.getScript(url.replace(/%ran%/,Math.random()),function(){
					paint();
				});
			},
			paint = function(){
				var i = 0, k = 0,
					lr = minestocks.length,
					lh = hots.length,
					l = lr + lh,
					h = [];
				h.push(tls);
				for(i = 0; i < l, k < num; i++, k++){
					var d,s,ttli = tli,
						symbol,cname,price,chg,ccolor;
					if(i < lr){
						symbol = minestocks[i];
						ttli = ttli.replace(/%func%/,tfunc);
					}else if(lr === num - 1){break;}
					else{symbol = hots[i - lr];}
					d = window['hq_str_s_' + symbol];
					if(i === lr && lh > 0){
						h.push(tlh);
						k ++;
					}
					if(!d){
						cname = symbol;
						price = '--';
						chg = '--';
						ccolor = '';
					}else{
						s = d.split(',');
						cname = s[0];
						price = (s[1] - 0).toFixed(2);
						chg = s[3] - 0;
						ccolor = chg > 0 ? 'cred' : chg < 0 ? 'cgreen' : '';
						chg = chg > 0 ? '+' + chg.toFixed(2) + '%' : chg.toFixed(2) + '%';
					}
					h.push(ttli.replace(/%symbol%/g,symbol).replace(/%cname%/g,cname).replace(/%ccolor%/g,ccolor).replace(/%price%/g,price).replace(/%chg%/g,chg));
				}
				h.push(tle);
				w.innerHTML = h.join('');
			},
			initStocks = function(){
				var symbols = minestocks.concat(hots),
					url = hqapi.replace(/%list%/,(function(){
						var ret = [];
						for(var i = 0, l = symbols.length; i < l; i++){
							ret.push('s_' + symbols[i]);
						}
						return ret.join(',');
					})());
				getStocks(url);
				interval && clearInterval(interval);
				interval = setInterval(function(){
					getStocks(url);
				},delay);
			},
			init = function(b){
				if(window.SINA_OUTLOGIN_LAYER && SINA_OUTLOGIN_LAYER.isLogin()){
					getMinestocks();
				}

				if(typeof loginSuccessCallbacks === 'undefined'){
					window['loginSuccessCallbacks'] = [];
				}
				!b && loginSuccessCallbacks.push(getMinestocks);
			};
		window.readMystocks = readMystocks;
		window.makeMyStocks = makeMyStocks;
		FN.delegate(document.getElementById('p01_r01_c02_list'),'list02_del','click',function(e,t){
			var p = FN.getParByTag(t,'li'),
				symbol = t.getAttribute('symbol') || '';
			p.parentNode.removeChild(p);
			FN.getScript(delmystockapi.replace(/%pid%/,pid).replace(/%symbol%/,symbol),function(){
				for(var i = 0, l = minestocks.length; i < l; i++){
					if(minestocks[i] === symbol){
						minestocks.splice(i,1);
						initStocks();
						break;
					}
				}
			});
		});
		init();
	})();
</script>
<!-- p01 end -->

<div style="margin-top:12px;"><ins class="sinaads" data-ad-pdps="PDPS000000055136"></ins></div> <script> (function (d, s, id) { var s, n = d.getElementsByTagName(s)[0]; if (d.getElementById(id)) return; s = d.createElement(s); s.id = id; s.setAttribute('charset', 'utf-8'); s.src = '//d' + Math.floor(0 + Math.random() * (9 - 0 + 1)) + '.sina.com.cn/litong/zhitou/sinaads/release/sinaads.js'; n.parentNode.insertBefore(s, n); })(document, 'script', 'sinaads-script'); (sinaads = window.sinaads || []).push({})</script>

