<script>
(function(){
	var a = [];
	if('indexOf' in a){return;}
	Array.prototype.indexOf = function(t){
		for(var i = 0, l = this.length; i < l; i++){
			if(this[i] === t){
				return i;
			}
		}
		return null;
	}
})();
</script>
