			},
			formatC5 = function(){
				var ret = [],
					s = window['hq_json_sinaindustry_up'];
				if(typeof s === 'undefined' || s === ''){return ret;}
				for(var i = 0, l = s.length; i < l; i++){
					var d = s[i].split(','),
						c = d[9] - 0,
						hc = d[5] - 0,
						ccolor = c > 0 ? 'cred' : c < 0 ? 'cgreen' : '',
						hccolor = hc > 0 ? 'cred' : hc < 0 ? 'cgreen' : '';
					ret.push({
						rank : i + 1,
						symbol : d[8],
						cname : d[12],
						scname : d[12].slice(0,5),
						chg : c.toFixed(2),
						ccolor : ccolor,
